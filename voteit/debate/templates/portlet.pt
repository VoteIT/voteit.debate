<tal:main xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="voteit.debate">
<div id="user_speaker_lists"><!-- Don't put anything here! --></div>
<script language="javascript" type="text/javascript">
  var user_speaker_list_url = "${request.resource_url(context, '_user_speaker_lists')}";
  $(document).ready(function () {
    reload_speaker_in_queue = ${request.meeting.get_field_value('reload_speaker_in_queue', 5) * 1000};
    reload_speaker_not_in_queue = ${request.meeting.get_field_value('reload_speaker_not_in_queue', 15) * 1000};
    load_user_speaker_lists();
  });
</script>
</tal:main>
