      metal:use-macro="load: ${api.template_dir}content.pt"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="voteit.debate">
<body>
    <h2 metal:fill-slot="page_heading" id="page_heading" i18n:translate="">Manage speaker list</h2>
    <div metal:fill-slot="content">

        <tal:form replace="structure userid_form">Userid form</tal:form>


        <a href="javascript:" class="buttonize" id="go_next_speaker">Start next</a>

        <div id="speaker_list"></div>


        <a id="meeting_url" style="display: none" href="${api.meeting_url}"></a>
    </div>
